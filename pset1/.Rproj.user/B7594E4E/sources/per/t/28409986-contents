library(tidyverse)



# parameters

a_L = 0.3
a_K = 0.2
a_M = 0.5
eta = -5
delta = 0.1
beta = 0.9
rho = 0.9

sigma_xi = 1
sigma_epsilon = 3
sigma_u = 2
p_k = 1
w = 1

N = 1000
t = 10

xi = matrix(rnorm(N*t, sigma_xi), N, t)


omega = matrix(0, N, t)

omega[, 1] = xi[,1]

for (i in 2:t) {
  
  omega[,i] = rho*omega[, i-1] + xi[,i]
}
